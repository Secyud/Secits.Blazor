@attribute [Route(Menu.Button)]

<ComboBox Items="SecitsDemoConsts.Themes"  @bind-Value="Theme">
    <ListItemTemplate>
        @context.ToString()
    </ListItemTemplate>
</ComboBox>

<SButton Theme="Theme" Size="Size">
    <AddTemplate>Button</AddTemplate>
</SButton>

@code
{
    private Theme Theme { get; set; }

    private Size Size { get; set; }
}
