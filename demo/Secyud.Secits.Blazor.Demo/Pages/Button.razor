@attribute [Route(Menu.Button)]

<ComboBox Items="SecitsDemoConsts.Themes" ValueField="u => u" ItemFinder="u =>Task.FromResult(u) " @bind-Value="Theme">
    <ListItemTemplate>
        @context.ToString()
    </ListItemTemplate>
</ComboBox>

<SButton Theme="Theme" Size="Size">
    <AddCustomTemplate>Button</AddCustomTemplate>
</SButton>

@code
{
    private Theme Theme { get; set; }

    private Size Size { get; set; }
}
